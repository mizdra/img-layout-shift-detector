{
  "extends": ["github>hatena/renovate-config"],
  "ignorePresets": ["github>hatena/renovate-config:schedule"],
  "regexManagers": [
    {
      "fileMatch": ["^\\.github/workflows/[^/]+\\.ya?ml$"],
      "matchStrings": ["uses: (?<depName>.*?)@(?<currentValue>.*?)\\s"],
      "datasourceTemplate": "github-tags"
    }
  ]
}
